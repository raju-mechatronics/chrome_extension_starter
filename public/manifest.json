{
    "manifest_version": 3,
    "name": "OFFER_URL_SHORTNER",
    "version": "1.0",
    "description": "This extension will shorten the URL",
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://studio.youtube.com/video/*/edit*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}