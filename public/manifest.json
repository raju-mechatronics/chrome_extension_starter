{
    "manifest_version": 3,
    "name": "PDF 2 Form",
    "version": "1.0",
    "description": "Read PDF and fill form with it.",
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://studio.youtube.com/video/*/edit*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}